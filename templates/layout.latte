{contentType text/html}
{layout template('html.latte') }
{default $siteName = ''}

{block body}
    <h1>{ifset title}{include title}{else}Untitled{/ifset}</h1>
    <main>
        {block content}{/block}
    </main>
    <aside>
        {block sidebar}
            {include 'sidebar.latte'}
        {/block}
    </aside>
{/block}
